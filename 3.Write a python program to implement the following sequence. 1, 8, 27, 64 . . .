# Define a function to generate the sequence                                                                                                       R.HARITHA(192210406)
def cube_sequence(n):
    sequence = []
    for i in range(1, n + 1):
        sequence.append(i ** 3)
    return sequence

# Define how many terms you want in the sequence
num_terms = 4  # You can change this to generate more terms

# Generate and print the sequence
result = cube_sequence(num_terms)
print(result)



OUTPUT
[1, 8, 27, 64]
